{
  "name": "Page Info",
  "description" : "Domain IP address information",
  "version": "1.0",
  "browser_action": {
    "default_popup": "page_info.html",
    "default_icon": "pageInfo.png"
  },
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens page_info.html"
    }
  },
  "permissions" : [
	"tabs"
  ],
  "content_security_policy": "script-src 'self' https://maps.googleapis.com https://maps.gstatic.com; object-src 'self'"
}
